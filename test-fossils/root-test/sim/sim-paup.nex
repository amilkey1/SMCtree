#NEXUS

begin paup;
  log start file=pauplog.txt replace;
  exe sim.nex;
  set crit= like;
  lset nst=1 basefreq=equal rates=equal pinvar=0 clock userbrlen;
  gettrees file=sim.tre;
[!
*********** true tree ***********]
  describe 1 / plot=none brlens=sumonly;
  lset nouserbrlen;
  hsearch;
  savetrees file=mltree.tre brlen format=altnexus;
[!
*********** ML tree ***********]
  describe 1 / plot=none brlens=sumonly;
[!
*********** Tree distances ***********]
  lset userbrlen;
  gettrees file=sim.tre mode=3;
  gettrees file=mltree.tre mode=7;
  [gettrees file=beast/summary.tree mode=7;]
  treedist reftree=1 measure=pathlength file=pldist.txt replace;
  treedist reftree=1 measure=rfSymDiff file=rfdist.txt replace;
  log stop;
  quit;
end;
